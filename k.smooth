
d1 $ every 3 (0.25 <~) $ density 4 $ (striate' 4 0.6 $ sound "[odx odx, cp cp/1 cp/3 cp/4, odx/3 odx/6]")
  |+| speed "1"

d2 $ every 4 (0.5 <~) $ smash 4 [2,4,6,3] $ sound "gabba cp off ~"

d3 $ (every 3 (0.5 <~) $ slow 1 $ smash 8 [0.5,0.25,1,2] $ sound (samples "industrial h" (slow 5 "0 1 2 3 4 5"))
  |+| shape "0.1"
  |+| end "0.5")
  |+| speed (every 4 ((* 2) <$>) "8 16 4 2")
