hush

d1 $ every 11 (density 4) $ sound "bass1 ~ [bass1 bass1 [bassbass1 bd] ~] ~"
d1 silence

d2 $ sound "hc sn [sn hc hc ~]"
|+| vowel " i "

d3 $ every 14 (density 1 ) $ sound "[can/1*3]*2"
d3 silence

d4 $ every 4 (density 5) $ every 2 (density 2) $ sound "hc"
d4 silence

d5 $ every 10 (const $ sound "cp [cp ~ sn/1]")
d5 silence  
  
d6 $ slow 12 $ sound (pick <$> "rave" <*> ((floor . (*16)) <$> rand))
|+| speed "1"
d6 silence

hush
